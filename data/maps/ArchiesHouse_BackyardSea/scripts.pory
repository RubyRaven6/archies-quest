script ArchiesHouse_Backyard_EventScripts_Kyogre {
    lock
	faceplayer
    msgbox(format("Kyogre's vibing. He seems to be playing a ROMhack that has him on the cover.\pHe doesn't want to be disturbed."))
    closemessage
    applymovement(LOCALID_BACKYARD_SEA_KYOGRE, moves(face_original_direction))
    release
	end
}

